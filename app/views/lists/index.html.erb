<div class="container">
<div class="banner">
</div>
  <%= link_to "Create new list", new_list_path, class: "btn btn-danger"%>
  <ul>
  <div class="mt-3">
  <% @lists.each do |list| %>gi
    <div class="co-12 col-sm-6 col-md-4 mb-3">
        <%= link_to list_path(list), class:"text-decoration-none" do %>
          <div class="card-category" data-aos="zoom-in" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url()">
            <%= list.name %>
<%= console %>
          </div>
        <% end %>
      </div>
  <% end %>
  </div>
  </ul>
</div>
